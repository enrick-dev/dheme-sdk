{
  "name": "@dheme/next",
  "version": "1.6.0",
  "description": "Next.js App Router bindings for Dheme SDK with server-side theme generation",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "dheme",
    "theme",
    "nextjs",
    "app-router",
    "ssr",
    "shadcn",
    "tailwind",
    "fouc",
    "server-components"
  ],
  "author": "Dheme Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dheme/sdk"
  },
  "homepage": "https://dheme.com",
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@dheme/sdk": "^1.1.0",
    "@dheme/react": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "next": "^14.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
